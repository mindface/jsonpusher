{
	"name": "jsonpusher",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "yarn build:wasm && next dev",
		"build": "yarn build:wasm && next build",
		"start": "next start",
		"lint": "next lint",
		"storybook": "storybook dev -p 6006",
		"bfmt": "biome format ./src --write",
		"blint": "biome lint ./src",
		"bfl": "biome check --apply ./src",
		"test:e2e:all": "playwright test",
		"test:e2e": "playwright test --",
		"test-storybook": "test-storybook",
		"clean:wasm": "rm -rf public/wasm",
		"build:wasm": "yarn clean:wasm && cd wasm && wasm-pack build --target web --out-dir ../public/wasm --out-name wasm_calculator"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.32.1",
		"@auth/firebase-adapter": "^2.7.4",
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
		"@fullcalendar/core": "^6.1.15",
		"@fullcalendar/daygrid": "^6.1.15",
		"@fullcalendar/react": "^6.1.15",
		"@google/generative-ai": "^0.21.0",
		"@next-auth/firebase-adapter": "0.0.2-canary.207",
		"dayjs": "^1.11.13",
		"firebase": "^11.1.0",
		"firebase-admin": "^13.0.2",
		"next": "^15.1.2",
		"next-auth": "^5.0.0-beta.25",
		"nodemailer": "^6.9.15",
		"quill": "2.0.3",
		"react": "^18",
		"react-dom": "^18",
		"zustand": "^5.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@chromatic-com/storybook": "^1.9.0",
		"@playwright/test": "^1.48.2",
		"@storybook/addon-essentials": "^8.3.5",
		"@storybook/addon-interactions": "^8.3.5",
		"@storybook/addon-links": "^8.3.5",
		"@storybook/addon-onboarding": "^8.3.5",
		"@storybook/blocks": "^8.3.5",
		"@storybook/builder-vite": "8.3.5",
		"@storybook/nextjs": "^8.3.5",
		"@storybook/react": "^8.3.5",
		"@storybook/test": "^8.3.5",
		"@svgr/webpack": "^8.1.0",
		"@testing-library/react": "^16.0.1",
		"@types/next-auth": "^3.15.0",
		"@types/node": "^20",
		"@types/nodemailer": "^6",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"@wasm-tool/wasm-pack-plugin": "^1.7.0",
		"eslint": "^8",
		"eslint-config-next": "14.2.15",
		"eslint-plugin-storybook": "^0.9.0",
		"postcss": "^8",
		"storybook": "^8.3.5",
		"tailwindcss": "^3.4.1",
		"typescript": "^5",
		"vite": "^5.4.9",
		"wasm-pack": "^0.12.1"
	},
	"packageManager": "yarn@4.5.0"
}
